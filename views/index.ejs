    </div>
    <!-- for the stories part -->
    <div
        class="h-auto flex flex-row flex-nowrap items-center justify-start gap-2 w-full p-5 mt-20 overflow-x-scroll scrollbar-hide max-w-3xl">
        <!-- FOR A PARTICULAR PROFILES  -->
        <!-- for the admin div -->
        <div
            class="h-20 w-20 bg-gradient-to-b from-[#f4ff26] to-[#bee84a] rounded-full p-1 flex items-center justify-center flex-shrink-0 relative cursor-pointer">
            <img id="storiesView" class="bg-white h-full w-full object-cover rounded-full" src="<%=user.profileimg%>"
                alt="IMG">
            <div id="storiesUpload"
                class="h-6 w-6 absolute bg-gradient-to-b from-[#f4ff26] to-[#bee84a]  rounded-full bottom-0 right-0 flex items-center justify-center flex-shrink-0">
                <svg xmlns="http://www.w3.org/2000/svg" height="20" width="17.5"
                    viewBox="0 0 448 512"><!--!Font Awesome Free 6.7.2 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free Copyright 2025 Fonticons, Inc.-->
                    <path fill="#000000"
                        d="M256 80c0-17.7-14.3-32-32-32s-32 14.3-32 32l0 144L48 224c-17.7 0-32 14.3-32 32s14.3 32 32 32l144 0 0 144c0 17.7 14.3 32 32 32s32-14.3 32-32l0-144 144 0c17.7 0 32-14.3 32-32s-14.3-32-32-32l-144 0 0-144z" />
                </svg>
            </div>
        </div>
        <% if (stories.length>0) { %>
            <% stories.forEach(element=> { %>
                <div id="<%=element.StoryLink %>" class="stories cursor-pointer h-20 w-20 bg-gradient-to-b from-[#f4ff26] to-[#bee84a] rounded-full p-1 flex items-center justify-center flex-shrink-0">
                    <img src="<%=element.Ownerimg%>" class="bg-white h-full w-full object-cover rounded-full" alt="IMG">
                </div>
            <% }) %>          
        <% } %>

    </div>
    <!-- for the content part -->
    <div
        class="flex justify-start items-center flex-1 w-full overflow-y-scroll flex-col gap-1 max-w-2xl scrollbar-hide p-3 bg-gray-100 pb-20">
        <!-- content 1 -->
        <% posts.forEach((element,index)=> { %>
            <div class="w-full h-auto border border-gray-300 rounded-xl p-3 flex flex-col gap-2 bg-white">
                <!-- for the top part -->
                <div class="h-20 w-full rounded-lg flex items-center justify-center flex-row flex-shrink-0">
                    <div class="h-10 w-10 flex-shrink-0 overflow-hidden rounded-full">
                        <img loading="lazy" class="h-full w-full object-cover" src="<%=currentuser[index].profileimg %>"
                            alt="">
                    </div>
                    <div class="flex-1 h-[80%] w-full p-2">
                        <div class="h-[50%] w-auto font-custom text-2xl px-1">
                            <%=currentuser[index].name %>
                        </div>
                        <div class="h-[50%] w-auto flex items-center justify-start flex-row gap-1">
                            <div class="text-[12px]">
                                <%=currentuser[index].username %>
                            </div>
                            <div class="text-xl">&middot;</div>
                            <div class="text-[12px]">1 min ago</div>
                            <div class="text-xl">&middot;</div>
                            <div class="text-[12px]"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"
                                    width="18" height="18" color="#000000" fill="none">
                                    <path
                                        d="M12 22C6.47715 22 2 17.5228 2 12C2 9.20746 3.14465 6.68227 4.99037 4.86802M12 22C11.037 21.2864 11.1907 20.4555 11.6738 19.6247C12.4166 18.3474 12.4166 18.3474 12.4166 16.6444C12.4166 14.9414 13.4286 14.1429 17 14.8571C18.6047 15.1781 19.7741 12.9609 21.8573 13.693M12 22C16.9458 22 21.053 18.4096 21.8573 13.693M21.8573 13.693C21.9511 13.1427 22 12.5771 22 12C22 7.11857 18.5024 3.05405 13.8766 2.17579M13.8766 2.17579C14.3872 3.11599 14.1816 4.23551 13.1027 4.66298C11.3429 5.3603 12.6029 6.64343 11.1035 7.4356C10.1038 7.96372 8.6044 7.83152 7.10496 6.24716C6.31517 5.41264 5.83966 4.95765 4.99037 4.86802M13.8766 2.17579C13.2687 2.06039 12.6414 2 12 2C9.26969 2 6.79495 3.09421 4.99037 4.86802"
                                        stroke="currentColor" stroke-width="1.5" stroke-linejoin="round" />
                                </svg></div>
                        </div>
                    </div>
                </div>
                <!-- for the image part -->
                <div class="flex-1 w-full bg-white border border-gray-200 rounded-lg overflow-hidden">
                    <img loading="lazy" class="h-full w-full aspect-video object-contain ml-auto mr-auto block"
                        src="<%=element.postURL %>" alt="img">
                </div>
                <!-- for the comments part -->
                <div class="flex-1 hidden w-full relative items-center justify-center flex-col flex-shrink-0 border-y border-gray-300 font-poppins">
                    <!-- for the typing comment -->
                    <div class="h-10 w-full absolute bottom-0 bg-[#101010] rounded-xl">
                        <div id="<%=element.id %>" class="flex-1 flex items-center justify-center flex-row px-2">
                            <input class="CommentMessage h-full w-full outline-none border-none bg-black text-white" placeholder="Write your Comment..." type="text" name="comment">
                            <div class="submitComment h-10 w-10 flex items-center justify-center cursor-pointer">
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" class="injected-svg" data-src="https://cdn.hugeicons.com/icons/sent-solid-standard.svg" xmlns:xlink="http://www.w3.org/1999/xlink" role="img" color="#ffffff">
                                    <path d="M22.3886 3.50934C22.9207 2.01947 21.4821 0.58092 19.9922 1.11301L2.24451 7.45149C0.477797 8.08246 0.630218 10.6298 2.45959 11.0455L9.59327 12.648L13.3909 8.73607C13.7756 8.3398 14.4087 8.33041 14.8049 8.7151C15.2012 9.09979 15.2106 9.73289 14.8259 10.1292L10.9106 14.1623L12.456 21.042C12.8718 22.8714 15.4191 23.0238 16.0501 21.2571L22.3886 3.50934Z" fill="#ffffff"></path>
                                </svg>
                            </div>
                        </div>
                    </div>
                    <!-- for the comments chat -->
                     <div class="h-10 w-full bg-gray-200 rounded-lg my-1 flex items-center justify-between px-2">
                        <div>Comments</div>
                        <div class=" cursor-pointer commentClose">
                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" class="injected-svg" data-src="https://cdn.hugeicons.com/icons/cancel-01-solid-standard.svg" xmlns:xlink="http://www.w3.org/1999/xlink" role="img" color="#000000">
                                <path d="M4.11654 18.116C3.62839 18.6041 3.62839 19.3956 4.11654 19.8838C4.6047 20.3719 5.39616 20.3719 5.88431 19.8838L19.8843 5.88376C20.3725 5.39561 20.3725 4.60415 19.8843 4.11599C19.3962 3.62784 18.6047 3.62784 18.1165 4.11599L4.11654 18.116Z" fill="#000000"></path>
                                <path d="M5.88434 4.11599C5.39619 3.62784 4.60473 3.62784 4.11657 4.11599C3.62842 4.60415 3.62842 5.39561 4.11657 5.88376L18.1166 19.8838C18.6047 20.3719 19.3962 20.3719 19.8843 19.8838C20.3725 19.3956 20.3725 18.6041 19.8843 18.116L5.88434 4.11599Z" fill="#000000"></path>
                            </svg>
                        </div>
                     </div>
                     <div class="flex-1 h-80 w-full space-y-1 overflow-scroll pb-10 scrollbar-hide my-1">
                        <!-- <div class="max-w-[90%] border border-gray-300 float-right clear-both rounded-lg p-2">right</div>
                        <div class="max-w-[90%] border border-gray-300 float-left clear-both rounded-lg p-2">left</div>
                        <div class="max-w-[90%] border border-gray-300 float-left clear-both rounded-lg p-2">left</div>
                        <div class="max-w-[90%] border border-gray-300 float-left clear-both rounded-lg p-2">left</div>
                        <div class="max-w-[90%] border border-gray-300 float-left clear-both rounded-lg p-2">left</div>
                        <div class="max-w-[90%] border border-gray-300 float-left clear-both rounded-lg p-2">left</div>
                        <div class="max-w-[90%] border border-gray-300 float-left clear-both rounded-lg p-2">left</div>
                        <div class="max-w-[90%] border border-gray-300 float-left clear-both rounded-lg p-2">left</div> -->
                     </div>
                 </div>
                <!-- for the bottom part -->
                <div class="h-20 w-full rounded-lg flex items-center justify-center flex-col flex-shrink-0 p-2">
                    <!-- for caption -->
                    <div class="h-[50%] w-full text-[15px] font-poppins">
                        <%=element.caption %>
                    </div>
                    <div class="h-[50%] w-full flex items-center justify-start flex-row gap-4">
                        <!-- for likes -->
                        <div class="userlikebutton h-full w-auto flex flex-row gap-2 items-center justify-center cursor-pointer">

                            <% if (like[index]) { %>
                                <img class="cursor-pointer" id="<%=element.id %>"
                                    src="https://cdn.tirabeauty.com/v2/billowing-snowflake-434234/tira-p/wrkr/organization/62d539deb767fec5c06847bd/theme/assets/wishlist-selected.e1d3324ddd4f0001c62e7a8472402679.svg"
                                    alt="img">
                                <% } else { %>
                                    <img class="cursor-pointer" id="<%=element.id%>"
                                        src="https://cdn.tirabeauty.com/v2/billowing-snowflake-434234/tira-p/wrkr/organization/62d539deb767fec5c06847bd/theme/assets/wishlist-icon.1345c266e68871fdd7b62ace4997580a.svg"
                                        alt="(img)">
                                    <% } %>
                                        <div class="h-auto text-[12px]">
                                            <%=TotalLikes[index]%>
                                        </div>
                        </div>

                        <!-- for comments -->
                        <div class="comments h-full w-auto flex flex-row gap-2 items-center justify-center cursor-pointer">
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="22" height="22"
                                color="#000000" fill="none">
                                <path d="M8 13.5H16M8 8.5H12" stroke="currentColor" stroke-width="1"
                                    stroke-linecap="round" stroke-linejoin="round" />
                                <path
                                    d="M6.09881 19C4.7987 18.8721 3.82475 18.4816 3.17157 17.8284C2 16.6569 2 14.7712 2 11V10.5C2 6.72876 2 4.84315 3.17157 3.67157C4.34315 2.5 6.22876 2.5 10 2.5H14C17.7712 2.5 19.6569 2.5 20.8284 3.67157C22 4.84315 22 6.72876 22 10.5V11C22 14.7712 22 16.6569 20.8284 17.8284C19.6569 19 17.7712 19 14 19C13.4395 19.0125 12.9931 19.0551 12.5546 19.155C11.3562 19.4309 10.2465 20.0441 9.14987 20.5789C7.58729 21.3408 6.806 21.7218 6.31569 21.3651C5.37769 20.6665 6.29454 18.5019 6.5 17.5"
                                    stroke="currentColor" stroke-width="1.5" stroke-linecap="round" />
                            </svg>
                            <div class="h-auto text-[12px]">
                                <%=element.Comment %>
                            </div>
                        </div>
                    </div>
                </div>
                
            </div>
            <% }) %>

    </div>
    <!-- for the bottom nav bar -->
    <div
        class="h-16 bg-white w-full fixed bottom-0 z-50 flex items-center justify-between flex-row gap-5 rounded-t-sm max-w-3xl">
        <!-- for the all explore menu -->
        <div class="flex flex-row items-center justify-around w-2/3">
            <div class="h-auto w-auto">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="30" height="30" color="#000000"
                    fill="none">
                    <path
                        d="M2 18C2 16.4596 2 15.6893 2.34673 15.1235C2.54074 14.8069 2.80693 14.5407 3.12353 14.3467C3.68934 14 4.45956 14 6 14C7.54044 14 8.31066 14 8.87647 14.3467C9.19307 14.5407 9.45926 14.8069 9.65327 15.1235C10 15.6893 10 16.4596 10 18C10 19.5404 10 20.3107 9.65327 20.8765C9.45926 21.1931 9.19307 21.4593 8.87647 21.6533C8.31066 22 7.54044 22 6 22C4.45956 22 3.68934 22 3.12353 21.6533C2.80693 21.4593 2.54074 21.1931 2.34673 20.8765C2 20.3107 2 19.5404 2 18Z"
                        stroke="currentColor" stroke-width="1.5" />
                    <path
                        d="M14 18C14 16.4596 14 15.6893 14.3467 15.1235C14.5407 14.8069 14.8069 14.5407 15.1235 14.3467C15.6893 14 16.4596 14 18 14C19.5404 14 20.3107 14 20.8765 14.3467C21.1931 14.5407 21.4593 14.8069 21.6533 15.1235C22 15.6893 22 16.4596 22 18C22 19.5404 22 20.3107 21.6533 20.8765C21.4593 21.1931 21.1931 21.4593 20.8765 21.6533C20.3107 22 19.5404 22 18 22C16.4596 22 15.6893 22 15.1235 21.6533C14.8069 21.4593 14.5407 21.1931 14.3467 20.8765C14 20.3107 14 19.5404 14 18Z"
                        stroke="currentColor" stroke-width="1.5" />
                    <path
                        d="M2 6C2 4.45956 2 3.68934 2.34673 3.12353C2.54074 2.80693 2.80693 2.54074 3.12353 2.34673C3.68934 2 4.45956 2 6 2C7.54044 2 8.31066 2 8.87647 2.34673C9.19307 2.54074 9.45926 2.80693 9.65327 3.12353C10 3.68934 10 4.45956 10 6C10 7.54044 10 8.31066 9.65327 8.87647C9.45926 9.19307 9.19307 9.45926 8.87647 9.65327C8.31066 10 7.54044 10 6 10C4.45956 10 3.68934 10 3.12353 9.65327C2.80693 9.45926 2.54074 9.19307 2.34673 8.87647C2 8.31066 2 7.54044 2 6Z"
                        stroke="currentColor" stroke-width="1.5" />
                    <path
                        d="M14 6C14 4.45956 14 3.68934 14.3467 3.12353C14.5407 2.80693 14.8069 2.54074 15.1235 2.34673C15.6893 2 16.4596 2 18 2C19.5404 2 20.3107 2 20.8765 2.34673C21.1931 2.54074 21.4593 2.80693 21.6533 3.12353C22 3.68934 22 4.45956 22 6C22 7.54044 22 8.31066 21.6533 8.87647C21.4593 9.19307 21.1931 9.45926 20.8765 9.65327C20.3107 10 19.5404 10 18 10C16.4596 10 15.6893 10 15.1235 9.65327C14.8069 9.45926 14.5407 9.19307 14.3467 8.87647C14 8.31066 14 7.54044 14 6Z"
                        stroke="currentColor" stroke-width="1.5" />
                </svg>
            </div>
            <div id="home" class="h-auto w-auto cursor-pointer">
                <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" viewBox="0 0 24 24" fill="none"
                    class="injected-svg" data-src="https://cdn.hugeicons.com/icons/home-11-stroke-standard.svg"
                    xmlns:xlink="http://www.w3.org/1999/xlink" role="img" color="#000000">
                    <path
                        d="M9.00239 16.999L9.00048 21.0005C9.00021 21.5526 8.55258 22 8.00048 22L5 22C3.89543 22 3 21.1046 3 20V10.065C3 9.45242 3.28074 8.87362 3.76182 8.49437L10.7618 2.97609C11.488 2.40361 12.512 2.40361 13.2382 2.97609L20.2382 8.49437C20.7193 8.87362 21 9.45243 21 10.065V20C21 21.1046 20.1046 22 19 22L16 22C15.4477 22 15 21.5523 15 21V17C15 15.8954 14.1046 15 13 15H11.0024C9.8982 15 9.00292 15.8948 9.00239 16.999Z"
