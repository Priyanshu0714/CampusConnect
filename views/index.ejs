        <!-- for the upload icon -->
        <div id="uploadPostIcon"
            class="absolute cursor-pointer h-20 w-20 top-0 left-1/2 -translate-x-1/2 -translate-y-1/2 rounded-full bg-white p-1">
            <div
                class="h-full w-full bg-gradient-to-b rounded-full from-[#f4ff26] to-[#bee84a] flex items-center justify-center flex-shrink-0">
