    <!-- for the search bar for the users -->
    <div id="searchpage"
        class="absolute hidden h-full w-full z-[100] bg-white p-2 items-center justify-center flex-col">
        <!-- for the search bar -->
        <div class="w-full h-14 font-bold text-4xl font-northwellfont flex items-center justify-between">
            <div>Search</div>
            <div id="closesearchbar" class="mr-5 cursor-pointer"><svg xmlns="http://www.w3.org/2000/svg" height="24"
                    width="24"
                    viewBox="0 0 384 512"><!--!Font Awesome Free 6.7.2 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free Copyright 2025 Fonticons, Inc.-->
                    <path fill="#000000"
